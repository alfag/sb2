{
  "name": "SharingBeer2",
  "version": "1.0.0",
  "description": "A platform to connect customers and breweries for the sale and distribution of craft beers.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build:css": "node scripts/build-tailwind.js",
    "build:css:watch": "node scripts/build-tailwind.js --watch",
    "dev:full": "npm run build:css && concurrently \"npm run build:css:watch\" \"npm run dev\"",
    "test": "./scripts/safe_test_runner.sh",
    "test:unsafe": "mocha --recursive --exit",
    "test:brewery-stats": "mocha test/test_brewery_stats.js --timeout 30000",
    "test:integration": "mocha test/test_integration_complete.js --timeout 60000",
    "test:performance": "node test/test_performance_cache.js",
    "test:frontend": "node test/test_frontend_e2e.js",
    "test:quick": "node test/quick_system_check.js",
    "test:all": "node test/run_all_tests.js",
    "security-check": "node scripts/securityCheck.js",
    "backup": "./scripts/backup_system.sh backup",
    "backup:pre-test": "./scripts/backup_system.sh pre-test",
    "backup:cleanup": "./scripts/backup_system.sh cleanup",
    "test:safe": "./scripts/backup_system.sh pre-test && npm run test:unsafe",
    "test:with-backup": "./scripts/backup_system.sh pre-test && npm run test:all",
    "test:ultra-safe": "./scripts/pre_test_verification.sh check && ./scripts/backup_system.sh pre-test && npm run test:unsafe",
    "pre-test:verify": "./scripts/pre_test_verification.sh check",
    "pre-test:quick": "./scripts/pre_test_verification.sh quick"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.1",
    "@bull-board/express": "^6.14.1",
    "@google-cloud/aiplatform": "^5.0.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "bcrypt": "^5.0.1",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.2.0",
    "helmet": "^4.4.1",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.7.1",
    "ml-random-forest": "^2.1.0",
    "mongoose": "^8.11.0",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "naughty-words": "^1.2.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "passport-amazon": "^1.0.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qrcode": "^1.4.4",
    "simple-update-notifier": "^2.0.0",
    "stripe": "^8.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "chai": "^4.4.1",
    "chai-http": "^5.1.2",
    "concurrently": "^9.2.1",
    "eslint": "^7.14.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.10",
    "puppeteer": "^24.31.0",
    "sinon": "^21.0.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "beer",
    "craft beer",
    "brewery",
    "e-commerce"
  ],
  "author": "Your Name",
  "license": "MIT"
}
